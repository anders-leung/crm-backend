  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\index.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Yarn Test",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeExecutable": "yarn",
      "cwd": "${workspaceFolder}",
      "runtimeArgs": [
        "test"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Script",
      "program": "${workspaceFolder}\\server\\option\\populateOptions",
      "env": {
        "NODE_ENV": "prod"
      }
    }
  ]
}